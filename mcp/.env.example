# Daily API (for telephony)
DAILY_API_KEY=

# Bot service URL (Cloud Run URL for paty-bot, or http://localhost:8080 for local dev)
BOT_SERVICE_URL=

# Authentication (required by default)
# Set MCP_API_KEY for Bearer token auth, or MCP_AUTH_DISABLED=true to disable
MCP_API_KEY=
# MCP_AUTH_DISABLED=true

# Encrypted secrets vault
# Master key used to derive encryption keys (required to enable vault)
SECRETS_MASTER_KEY=
# Admin key for bearer management (create/revoke/list bearers)
SECRETS_ADMIN_KEY=
# Optional: custom path for the SQLite database (defaults to mcp/data/vault.db)
# SECRETS_DB_PATH=
